@inherits RocketMod.RocketModTokens<Simplisity.SimplisityRazor>
@using DNNrocketAPI;
@using Simplisity;
@using RocketSettings;
@using DNNrocketAPI.Componants;

@AddProcessData("resourcepath", "/DesktopModules/DNNrocket/api/App_LocalResources/")
@AddProcessData("resourcepath", "/DesktopModules/DNNrocket/SystemData/App_LocalResources/")
@AddProcessData("resourcepath", "/DesktopModules/DNNrocket/AppThemes/App_LocalResources/")

@{
}

<span class="w3-section simplisity_buttons">
</span>


<div class="w3-container">


    <div id="selectfield" class="w3-modal">

        <div class="w3-modal-content w3-animate-top w3-card-4">
            <header class="w3-container w3-theme-d3">
                <h2>@ResourceKey("AppThemes.select")</h2>
            </header>
            <div class="w3-container">
                @foreach (var versionfolder in Model.List)
                {
                    <p>
                        <span class="w3-button w3-block w3-theme " >XXXXXXXXXX</span>
                    </p>
                }
            </div>
        </div>

    </div>



</div>

<script>

    function showFieldSelect() {
        $('#selectfield').show();
    }

    function hideFieldSelect() {
        $('#selectfield').hide();
    }

</script>