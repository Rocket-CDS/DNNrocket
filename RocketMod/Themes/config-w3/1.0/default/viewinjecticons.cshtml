{
 editiconcolor = " w3-pale-blue ";
}

}

    <div id="rocketmodcontentediticons@(Model.ModuleId)" class='w3-display-topleft w3-padding ' style='display:none;z-index:99;'>
        <a href='javascript:rocketmodeditiframe_@(Model.ModuleId)()' class=''><i class='w3-small @(editiconcolor) @(editicontextcolor) w3-border w3-circle w3-card-4 w3-padding fas fa-edit'></i></a>
